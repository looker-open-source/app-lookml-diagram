{
  "name": "lookml-diagram-extension",
  "sideEffects": [
    "*.css"
  ],
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "analyze": "export ANALYZE_MODE=static && yarn build",
    "build": "export BABEL_ENV=build && webpack --config webpack.prod.js",
    "develop": "webpack serve --hot --port 8080 --disable-host-check --config webpack.develop.js --https",
    "test": "jest",
    "build:ci:clean": "rm -rf ./temp && mkdir ./temp",
    "build:ci": "export BABEL_ENV=build && webpack --config webpack.prod.js --output-path=./temp",
    "lint": "eslint './src/**/*.ts{,x}' --cache",
    "lint-fix": "eslint './src/**/*.ts{,x}' --fix --cache"
  },
  "license": "MIT",
  "dependencies": {
    "@looker/components": "^3.0.8",
    "@looker/design-tokens": "^2.7.21",
    "@looker/extension-sdk": "^21.4.3",
    "@looker/extension-sdk-react": "^21.4.3",
    "@looker/icons": "1.5.21",
    "@looker/sdk": "^21.4.2",
    "@styled-icons/material": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "d3": "^6.3.0",
    "prism-react-renderer": "^1.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-json-inspector": "^7.1.1",
    "react-query": "^3.13.12",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "recharts": "^1.7.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@looker/components-test-utils": "^1.5.26",
    "@looker/eslint-config-oss": "1.7.14",
    "@looker/prettier-config": "^0.10.4",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.6",
    "@types/d3": "^6.2.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/recharts": "^1.1.21",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@webpack-cli/serve": "^1.3.1",
    "babel-loader": "^8.2.2",
    "babel-loader-exclude-node-modules-except": "^1.1.2",
    "babel-preset-nano-react-app": "^0.1.0",
    "core-js": "^3.11.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.0.0",
    "jest": "26.6.3",
    "jest-styled-components": "^7.0.3",
    "prettier": "2.2.1",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^17.0.1",
    "style-loader": "^1.1.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^6.2.2",
    "typescript": "^4.2.4",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintConfig": {
    "extends": [
      "@looker/eslint-config-oss"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "camelcase": "off",
      "@typescript-eslint/interface-name-prefix": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "args": "all",
          "argsIgnorePattern": "^_"
        }
      ],
      "sort-keys-fix/sort-keys-fix": "off",
      "no-useless-constructor": "off",
      "@typescript-eslint/no-empty-interface": "off",
      "import/default": "off",
      "sort-keys": "off",
      "spaced-comment": [
        "error",
        "always",
        {
          "markers": [
            "#region",
            "#endregion"
          ]
        }
      ],
      "no-use-before-define": "off",
      "no-console": 0,
      "header/header": [
        2,
        "block",
        [
          "",
          "",
          " MIT License",
          "",
          {
            "pattern": " Copyright \\(c\\) 20\\d{2} Looker Data Sciences, Inc.",
            "template": " Copyright (c) 2022 Looker Data Sciences, Inc."
          },
          "",
          " Permission is hereby granted, free of charge, to any person obtaining a copy",
          " of this software and associated documentation files (the \"Software\"), to deal",
          " in the Software without restriction, including without limitation the rights",
          " to use, copy, modify, merge, publish, distribute, sublicense, and/or sell",
          " copies of the Software, and to permit persons to whom the Software is",
          " furnished to do so, subject to the following conditions:",
          "",
          " The above copyright notice and this permission notice shall be included in all",
          " copies or substantial portions of the Software.",
          "",
          " THE SOFTWARE IS PROVIDED \"AS IS\", WITHOUT WARRANTY OF ANY KIND, EXPRESS OR",
          " IMPLIED, INCLUDING BUT NOT LIMITED TO THE WARRANTIES OF MERCHANTABILITY,",
          " FITNESS FOR A PARTICULAR PURPOSE AND NONINFRINGEMENT. IN NO EVENT SHALL THE",
          " AUTHORS OR COPYRIGHT HOLDERS BE LIABLE FOR ANY CLAIM, DAMAGES OR OTHER",
          " LIABILITY, WHETHER IN AN ACTION OF CONTRACT, TORT OR OTHERWISE, ARISING FROM,",
          " OUT OF OR IN CONNECTION WITH THE SOFTWARE OR THE USE OR OTHER DEALINGS IN THE",
          " SOFTWARE.",
          "",
          " "
        ],
        1
      ]
    },
    "settings": {
      "import/resolver": {
        "typescript": {
          "project": "./tsconfig.json"
        }
      },
      "import/external-module-folders": [
        "node_modules"
      ]
    },
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "rules": {
          "@typescript-eslint/no-var-requires": "off"
        }
      }
    ]
  },
  "prettier": "@looker/prettier-config",
  "prettierConfig": {
    "overrides": {
      "rules": {
        "trailingComma": "all"
      }
    }
  },
  "resolutions": {
    "trim": ">= 0.0.3",
    "css-what": ">= 5.0.1",
    "glob-parent": ">= 5.1.2",
    "ws": ">= 7.4.6",
    "dns-packet": ">= 1.3.2",
    "node-forge": ">= 1.3.0"
  }
}
